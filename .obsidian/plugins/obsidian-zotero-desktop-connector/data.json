{
  "database": "Zotero",
  "noteImportFolder": "Zotero",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "formatted-citation",
      "cslStyle": "apa"
    }
  ],
  "exportFormats": [
    {
      "name": "Zotero Import",
      "outputPathTemplate": "Inputs/Zotero/{{citekey}}.md",
      "imageOutputPathTemplate": "Inputs/Zotero/{{citekey}}/Images",
      "imageBaseNameTemplate": "image",
      "templatePath": "_system/Templates/_Plugin Templates/Zotero Template.md",
      "cslStyle": "apa"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note"
}